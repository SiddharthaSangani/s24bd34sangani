extends layout

block content
  h1= title
  - let results = ['rock','loin','Sand','bear','clay','wolves']
  div(class="container-fluid")
    div(class="row")
      each item in results  
        div(class="col-sm-2",onclick=`buy(${index})`)
          p #{item}     
    script.
      function buy(index) {
        alert(`You purchased item ${index}`);}